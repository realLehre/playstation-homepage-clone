ht@100;300;400;500;700;900&display=swap');\r\n\r\n$dark-color: 99;\r\n$blue-color: #0568be;\r\n$gray-color: rgba(0, 0, 0, 0.5);\r\n$gray-darker-color: rgba(0, 0, 0, 0.8);\r\n\r\n$font-sizes: ();\r\n\r\n@mixin mainFont {\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n*,\r\n*::after,\r\n*::before {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  scroll-behavior: smooth;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n  color: $dark-color;\r\n}\r\n\r\nli {\r\n  list-style: none;\r\n}\r\n\r\n@mixin flex($jus, $align, $dir) {\r\n  display: flex;\r\n  justify-content: $jus;\r\n  align-items: $align;\r\n  flex-direction: $dir;\r\n}\r\n\r\n@mixin transition {\r\n  transition: all 700ms ease-in-out;\r\n}\r\n","src\\scss\\_breakpoints.scss":"@media (max-width: 700px) {\r\n  #sec-header {\r\n    display: none;\r\n  }\r\n\r\n  .header-item.item1 {\r\n    display: block;\r\n  }\r\n\r\n  .links-container {\r\n    // position: absolute;\r\n    display: none;\r\n  }\r\n\r\n  .signin-container {\r\n    & i.fa-search {\r\n      display: none;\r\n    }\r\n  }\r\n}\r\n","src\\scss\\main.scss":"@use 'config' as c;\r\n@use 'breakpoints';\r\n\r\nbody {\r\n  @include c.mainFont;\r\n  font-size: 10px;\r\n  // background: gray;\r\n}\r\n\r\n#sec-header {\r\n  width: 100%;\r\n  background: black;\r\n  padding: 10px 0;\r\n  padding-right: 5px;\r\n}\r\n\r\n.sec-header {\r\n  @include c.flex(flex-end, flex-end, column);\r\n\r\n  & .logo {\r\n    width: 75px;\r\n\r\n    & img {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n#pri-header {\r\n  width: 100%;\r\n  height: auto;\r\n  background: white;\r\n}\r\n\r\n.pri-header {\r\n  position: relative;\r\n  width: 100%;\r\n  height: auto !important;\r\n  margin-inline: auto;\r\n  @include c.flex(space-between, center, row);\r\n}\r\n\r\n.item1 {\r\n  display: none;\r\n  margin-left: 30px;\r\n  & i {\r\n    font-size: 20px;\r\n    cursor: pointer;\r\n    &:not(:last-child) {\r\n      margin-right: 35px;\r\n    }\r\n  }\r\n}\r\n\r\n.header-item.item2 {\r\n  @include c.flex(space-between, center, row);\r\n  margin-left: 30px;\r\n}\r\n\r\n.links-container {\r\n  margin-left: 10px;\r\n  & .links {\r\n    @include c.flex(space-between, center, row);\r\n\r\n    & li {\r\n      @include c.flex(space-between, flex-end, row);\r\n      font-size: 1.4em;\r\n      font-weight: 500;\r\n      cursor: pointer;\r\n      transition: all 200ms;\r\n\r\n      &:not(:last-child) {\r\n        margin-right: 10px;\r\n      }\r\n\r\n      &:hover {\r\n        color: c.$blue-color;\r\n        & i {\r\n          color: c.$blue-color;\r\n        }\r\n      }\r\n\r\n      & i {\r\n        transform: translateY(4px);\r\n        margin-left: 5px;\r\n        font-size: 18px;\r\n        font-weight: bolder;\r\n        color: rgba(0, 0, 0, 0.5);\r\n        transition: all 200ms;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.links-tabs-container {\r\n  position: absolute;\r\n  width: 100%;\r\n  top: 70px;\r\n}\r\n\r\n.link-tab {\r\n  width: 100%;\r\n  @include c.flex(center, center, column);\r\n\r\n  & .perks-array {\r\n    @include c.flex(center, flex-end, row);\r\n\r\n    & .perk-item {\r\n      @include c.flex(center, center, column);\r\n\r\n      &:not(:last-child) {\r\n        margin-right: 60px;\r\n      }\r\n\r\n      & img {\r\n        width: 55px;\r\n        margin-bottom: 10px;\r\n      }\r\n\r\n      & p {\r\n        text-align: center;\r\n        font-size: 1.4em;\r\n        font-weight: 400;\r\n      }\r\n    }\r\n  }\r\n\r\n  .link-tab-ad-info {\r\n    width: 100%;\r\n    @include c.flex(center, center, row);\r\n    margin-top: 30px;\r\n    box-shadow: -0.5px -0.5px 0px rgba(0, 0, 0, 0.3);\r\n    padding: 15px 0;\r\n\r\n    & .info {\r\n      @include c.flex(center, center, row);\r\n      font-size: 1.5em;\r\n      cursor: pointer;\r\n\r\n      & .dot {\r\n        display: inline-block;\r\n        width: 8px;\r\n        height: 8px;\r\n        background-color: c.$blue-color;\r\n        border-radius: 50%;\r\n        margin-right: 8px;\r\n      }\r\n\r\n      &:not(:last-child) {\r\n        margin-right: 30px;\r\n      }\r\n\r\n      &:hover {\r\n        color: c.$blue-color;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.signin-container {\r\n  @include c.flex(space-between, center, row);\r\n  margin-right: 30px;\r\n\r\n  & .signin {\r\n    background: c.$gray-darker-color;\r\n    color: white;\r\n    padding: 4px 10px;\r\n    border-radius: 50px;\r\n    font-size: 1.7em;\r\n    text-transform: capitalize;\r\n    cursor: pointer;\r\n\r\n    &:hover {\r\n      background: c.$gray-color;\r\n    }\r\n  }\r\n\r\n  & i.fa-search {\r\n    margin-left: 25px;\r\n    font-size: 18px;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n// @use 'breakpoints';\r\n"},"lineCount":null}},"error":null,"hash":"9fcac5c4839598fe5d74d98fca039cef","cacheData":{"env":{}}}