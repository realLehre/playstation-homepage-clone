st-child) {\r\n        margin-right: 10px;\r\n      }\r\n\r\n      &:hover {\r\n        color: c.$blue-color;\r\n        & i {\r\n          color: c.$blue-color;\r\n        }\r\n      }\r\n\r\n      & i {\r\n        transform: translateY(4px);\r\n        margin-left: 5px;\r\n        font-size: 18px;\r\n        font-weight: bolder;\r\n        color: rgba(0, 0, 0, 0.5);\r\n        transition: all 200ms;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.links-tabs-container {\r\n  @include c.flex(space-around, center, row);\r\n  width: 70%;\r\n  position: absolute;\r\n  top: 90px;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n}\r\n\r\n.link-tab {\r\n  @include c.flex(center, center, column);\r\n\r\n  & .perks-array {\r\n    @include c.flex(center, center, row);\r\n  }\r\n}\r\n\r\n.signin-container {\r\n  @include c.flex(space-between, center, row);\r\n\r\n  & .signin {\r\n    background: c.$gray-darker-color;\r\n    color: white;\r\n    padding: 4px 10px;\r\n    border-radius: 50px;\r\n    font-size: 1.7em;\r\n    text-transform: capitalize;\r\n    cursor: pointer;\r\n\r\n    &:hover {\r\n      background: c.$gray-color;\r\n    }\r\n  }\r\n\r\n  & i.fa-search {\r\n    margin-left: 25px;\r\n    font-size: 18px;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n// @use 'breakpoints';\r\n"},"lineCount":null}},"error":null,"hash":"d4a3c0529f4c8476ac7822ec2f6fec14","cacheData":{"env":{}}}